    }                                        

    private void jComboBox1ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        if( jComboBox1.getSelectedItem().equals("PD10")){
        jComboBox2.setSelectedItem("Tomato Sauce");
        jTextField1.setText("Heinz sauce");
        jComboBox6.setSelectedItem("250");
        
        }
        
        if(jComboBox1.getSelectedItem().equals("PD11")){
         jComboBox2.setSelectedItem("Salt");
          jTextField1.setText("Kensalt");
          jComboBox6.setSelectedItem("100");
         
        }
        
        if(jComboBox1.getSelectedItem().equals("PD12")){
        jComboBox2.setSelectedItem("Sugar");
        jTextField1.setText("Normal sugar");
        jComboBox6.setSelectedItem("150");
        }
        
        if(jComboBox1.getSelectedItem().equals("PD13")){
        jComboBox2.setSelectedItem("Guiness");
        jTextField1.setText("EABL");
        jComboBox6.setSelectedItem("180");
        }
        if(jComboBox1.getSelectedItem().equals("PD14")){
          jComboBox2.setSelectedItem("Tusker");
          jTextField1.setText("EABL");
          jComboBox6.setSelectedItem("180");
           }
       
        
    }                                          

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
         jTextField1.setText(null);
           jTextField2.setText(null);
             jTextField3.setText(null);
              jTextField4.setText(null);
               jTextField5.setText(null);
               jComboBox1.setSelectedItem("Select Product ID");
               jComboBox2.setSelectedItem("Select Product Name");
               jComboBox3.setSelectedItem("Select Employee ID");
               jComboBox4.setSelectedItem("Select FName");
               jComboBox5.setSelectedItem("Select LName");
               
               
               
              
              
           
         
    }                                        

