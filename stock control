    }                                           

    private void jTextField5ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        product[0]= Double.parseDouble(jTextField2.getText());
        product[1]=Double.parseDouble(jTextField3.getText());
        
        double total1= (product[1]-product[0]);
        String cost1=String.valueOf(total1);
        jTextField7.setText(cost1);
        
    }                                           

