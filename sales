    private void jCheckBox2MouseClicked(java.awt.event.MouseEvent evt) {                                        
        // TODO add your handling code here:
        double cMeal= Double.parseDouble(jTextFieldcMeal.getText());
        double cBurger= Double.parseDouble(jTextField1.getText());
        double ccBurger=250;
        
        if( jCheckBox1.isSelected()){
        double meal1=(cBurger*ccBurger)+cMeal;
        String pMeal= String.format("Ksh",meal1);
        jTextFieldcMeal.setText(pMeal);
        }
        
    }                                       

    private void jCheckBox3MouseClicked(java.awt.event.MouseEvent evt) {                                        
        // TODO add your handling code here:
          double cMeal= Double.parseDouble(jTextFieldcMeal.getText());
        double cBaconFries= Double.parseDouble(jTextField1.getText());
        double ccBaconFries=220;
        
        if( jCheckBox2.isSelected()){
        double meal1=(cBaconFries*ccBaconFries)+cMeal;
        String pMeal= String.format("Ksh",meal1);
        jTextFieldcMeal.setText(pMeal);
    }                                       
    }
