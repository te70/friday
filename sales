        }
        if(jComboBox2.getSelectedItem().equals("Bacon Fries")){
        String baconFries= String.format("220");
            jTextFieldcMeal.setText(baconFries);
        }
         if(jComboBox2.getSelectedItem().equals("Chicken Fries")){
        String chickenFries= String.format("200");
            jTextFieldcMeal.setText(chickenFries);
        
    }                                          
     if(jComboBox2.getSelectedItem().equals("Plain Fries")){
        String plainFries= String.format("120");
            jTextFieldcMeal.setText(plainFries);
          }
          
          
            
    }                                          

