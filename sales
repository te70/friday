        double qty2= Double.parseDouble(jTextField5.getText());
        double qty3= Double.parseDouble(jTextField7.getText());
        double qty4= Double.parseDouble(jTextField9.getText());
        
        jTextArea1.append("\tFriday Management Systems:\n\n" + "\nCost of Meal:\t\t"
        +qty1+"\nCost of Drinks:\t\t"+qty2+"\nSub Total:\t\t"+qty3+"\nTotal:\t\t"+qty4+"\n\nThanks for using our System"
        
        );
        
