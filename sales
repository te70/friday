    private void jCheckBox6ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
          double cMeal= Double.parseDouble(jTextFieldcMeal.getText());
        double cFive= Double.parseDouble(jTextField5.getText());
        double iTax=20;
        
        if( jCheckBox1.isSelected()){
        double tax=(cFive+cMeal)/iTax;
        String pTax= String.valueOf(tax);
        jTextField8.setText(pTax);
    }                                       
    }                                          

