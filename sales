    private void jComboBox1ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        double minuteMaid=100;
        double sodaCoke=100;
        double sodaPepsi=80;
        double freshJuice=70;
        double yoghurt=80;
        double beer=180;
        if(jComboBox1.getSelectedItem().equals("Minute maid")){
        String cMinuteMaid= String.format("100");
            jTextField5.setText(cMinuteMaid);
        }
        if(jComboBox1.getSelectedItem().equals("Soda(Coke)")){
        String cSodaCoke= String.format("100");
            jTextField5.setText(cSodaCoke);
        }
         if(jComboBox1.getSelectedItem().equals("Fresh Juice")){
        String cFreshJuice= String.format("70");
            jTextField5.setText(cFreshJuice);
        
    }                                          
     if(jComboBox1.getSelectedItem().equals("Soda(Pepsi)")){
        String cSodaPepsi= String.format("80");
            jTextField5.setText(cSodaPepsi);
          }
          
           if(jComboBox1.getSelectedItem().equals("Yoghurt")){
        String cYoghurt= String.format("100");
            jTextField5.setText(cYoghurt);
           }
           
            if(jComboBox1.getSelectedItem().equals("Beer")){
        String cBeer= String.format("180");
            jTextField5.setText(cBeer);
            }
            
    }
            
